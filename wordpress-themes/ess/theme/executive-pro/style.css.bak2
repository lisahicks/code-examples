	.genesis-nav-menu.responsive-menu .menu-open.menu-item-has-children:before {

		content: "\f142";

	}

	

	.genesis-nav-menu.responsive-menu .sub-menu {

		border: none;
		left: auto;
		opacity: 1;
		padding-left: 20px;
		position: relative;
		width: 100%;
		z-index: 99;
	}
	
	.genesis-nav-menu ul.sub-menu li.menu-item-has-children > a:after { 
