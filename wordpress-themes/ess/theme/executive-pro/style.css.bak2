---------------------------------------------------------------------------------------------------- */



.genesis-nav-menu {

	max-height: 50px;

	clear: both;

	color: #fff;

	font-size: 14px;

	line-height: 1.5;

	margin: 0;

	overflow: hidden;

	padding: 0;

	width: 100%;

	text-transform: uppercase;

	background: #393939 url(http://dev-ww.com/ess/images/bg-header.gif) repeat;

    background-position-x: initial;

    background-position-y: initial;

    background-size: initial;

    background-repeat-x: repeat;

    background-repeat-y: repeat;

    background-attachment: initial;

    background-origin: initial;

    background-clip: initial;

}



.genesis-nav-menu .menu-item {

	display: inline-block;

	margin: 0;

	padding: 0;

	text-align: left;

}



.genesis-nav-menu a {

	color: #fff;

	display: block;

	padding: 16px 20px;

	position: relative;

	text-decoration: none;

}



.genesis-nav-menu > li:hover,

.genesis-nav-menu > a:hover {

	color: #fff;

	background-color: rgba(44, 116, 180, 0.80);

	border-top-left-radius: 10px;

	border-top-right-radius: 10px;

}



.genesis-nav-menu .sub-menu {
	left: -9999px;
	margin: -3px 0 0 0;
	opacity: 0;
	padding: 0;
	position: absolute;
	-webkit-transition: opacity .4s ease-in-out;
	-moz-transition:    opacity .4s ease-in-out;
	-ms-transition:     opacity .4s ease-in-out;
	-o-transition:      opacity .4s ease-in-out;
	transition:         opacity .4s ease-in-out;
	width: 165px;
	z-index: 999;

}
.genesis-nav-menu > .menu-item > ul.sub-menu { 
	background-color: rgba(44, 116, 180, 0.80);
	border-top-right-radius: 10px;
	border-bottom-left-radius: 10px;
	border-bottom-right-radius: 10px;
}
.genesis-nav-menu ul.sub-menu > li > ul.sub-menu > li { 
	width: 165px;
	background-color: rgba(113, 112, 115, 0.70) !important;
}

.genesis-nav-menu ul.sub-menu > li > ul.sub-menu > li:first-child { 
	border-top-right-radius: 10px;
	border-top-left-radius: 10px;
}

.genesis-nav-menu ul.sub-menu > li > ul.sub-menu > li:last-child { 
	border-bottom-left-radius: 10px;
	border-bottom-right-radius: 10px;
}
.genesis-nav-menu .sub-menu a {
	border: none !important;
	color: #fff;
	font-size: 12px;
	padding: 10px 18px;
	position: relative;
	width: 148px;
}
.genesis-nav-menu .sub-menu a:hover {
	color: #000 !important;
	font-weight: bold;
}



.genesis-nav-menu ul.sub-menu li.menu-item-has-children > a:after { 

    content: "";

    width: 0; 

    height: 0; 

    border-left: 8px solid #fff;

    border-right: 8px solid transparent;

    border-top: 8px solid transparent;

	border-bottom: 8px solid transparent;

    position: absolute;

    top: 10px;

    right: -10px;

}



.genesis-nav-menu ul.sub-menu:hover li.menu-item-has-children > a:after { 

    border-left: 8px solid #000 !important;

}



.genesis-nav-menu .sub-menu a:hover span {

	color: #000 !important;

}



.genesis-nav-menu .sub-menu .sub-menu {

	margin: -40px 0 0 147px;

}



.genesis-nav-menu .menu-item:hover {

	position: static;

}



.genesis-nav-menu .menu-item:hover > .sub-menu {

	left: auto;

	opacity: 1;

}



.genesis-nav-menu > .first > a {

	padding-left: 0;

}



.genesis-nav-menu > .last > a {

	padding-right: 0;

}



.genesis-nav-menu > .right {

	display: inline-block;

	float: right;

	list-style-type: none;

	padding: 16px 20px;

}



.genesis-nav-menu > .right > a {

	display: inline;

	padding: 0;

}



