		margin: 0;
	}

	.genesis-nav-menu.responsive-menu .sub-menu li a,

	.genesis-nav-menu.responsive-menu .sub-menu li a:hover {

		padding: 12px 16px;

		position: relative;

		text-transform: none;

		width: 100%;

		color: #000 !important;

		font-weight: bold;

	}
	.genesis-nav-menu.responsive-menu .sub-menu li, .genesis-nav-menu.responsive-menu .sub-menu li a {
		background-color: rgba(44, 116, 180, 1.0) !important;
	}
	.genesis-nav-menu.responsive-menu .sub-menu li:hover, .genesis-nav-menu.responsive-menu .sub-menu li a:hover {
		background-color: rgba(113, 112, 115, 1.0) !important;
	}
/*Prevent sub menu from inheirting when not parent is in hover state*/
