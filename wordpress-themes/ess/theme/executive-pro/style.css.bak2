/* Tables

--------------------------------------------- */



table {

	border-collapse: collapse;

	border-spacing: 0;

	line-height: 2;

	margin-bottom: 40px;

	width: 100%;

}



tbody {

	border-bottom: 1px solid #ddd;

}



